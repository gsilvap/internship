    }
    atexit(apr_terminate);

    if (argc) {
        shortname = apr_filepath_name_get(argv[0]);
    }

    if (apr_pool_create_ex(&pool, ((void *)0), ((void *)0), ((void *)0)) != 0) {
        return 1;
    }
    apr_pool_abort_set(oom, pool);
    apr_file_open_stderr(&errfile, pool);
    apr_signal(2, setterm);
    apr_signal(15, setterm);

    apr_getopt_init(&o, pool, argc, argv);

    while (1) {
        status = apr_getopt(o, "iDnvrtd:l:L:p:", &opt, &arg);
        if (status == ((20000 + 50000) + 14)) {
            break;
        }
        else if (status != 0) {
            usage();
        }
        else {
            switch (opt) {
            case 'i':
                if (intelligent) {
                    usage();
                }
                intelligent = 1;
                break;

            case 'D':
                if (dryrun) {
                    usage();
                }
                dryrun = 1;
                break;

            case 'n':
                if (benice) {
                    usage();
                }
                benice = 1;
                break;

            case 't':
                if (deldirs) {
                    usage();
                }
                deldirs = 1;
                break;

            case 'v':
                if (verbose) {
                    usage();
                }
                verbose = 1;
                break;

            case 'r':
                if (realclean) {
                    usage();
                }
                realclean = 1;
                deldirs = 1;
                break;

            case 'd':
                if (isdaemon) {
                    usage();
                }
                isdaemon = 1;
                repeat = apr_atoi64(arg);
                repeat *= 60;
                repeat *= 1000000L;
                break;

            case 'l':
                if (limit_found) {
                    usage();
                }
                limit_found = 1;

                do {
                    apr_status_t rv;
                    char *end;

                    rv = apr_strtoff(&max, arg, &end, 10);
                    if (rv == 0) {
                        if ((*end == 'K' || *end == 'k') && !end[1]) {
                            max *= 1024;
                        }
                        else if ((*end == 'M' || *end == 'm') && !end[1]) {
                            max *= 1048576;
                        }
                        else if ((*end == 'G' || *end == 'g') && !end[1]) {
                            max *= 1073741824;
                        }
                        else if (*end &&
                                 ((*end != 'B' && *end != 'b') || end[1])) {
                            rv = (20000 + 14);
                        }
                    }
                    if (rv != 0) {
                        apr_file_printf(errfile, "Invalid limit: %s"
                                                 "\n" "\n", arg);
                        usage();
                    }
                } while(0);
                break;

            case 'p':
                if (proxypath) {
                    usage();
                }
                proxypath = apr_pstrdup(pool, arg);
                if (apr_filepath_set(proxypath, pool) != 0) {
                    usage();
                }
                break;
            }
        }
    }

    if (o->ind != argc) {
         usage();
    }

    if (isdaemon && (repeat <= 0 || verbose || realclean || dryrun)) {
         usage();
    }

    if (!isdaemon && intelligent) {
         usage();
    }

    if (!proxypath || max <= 0) {
         usage();
    }

    if (apr_filepath_get(&path, 0, pool) != 0) {
        usage();
    }
    baselen = strlen(path);


    if (isdaemon) {
        apr_file_close(errfile);
        apr_proc_detach(1);
    }


    do {
        apr_pool_create_ex(&instance, pool, ((void *)0), ((void *)0));

        now = apr_time_now();
        do { ((&root))->next = (struct _entry *)((char *)(&((&root))->next) - ((long) (((char *) (&(((struct _entry*)((void *)0))->link))) - ((char *) ((void *)0))))); ((&root))->prev = (struct _entry *)((char *)(&((&root))->next) - ((long) (((char *) (&(((struct _entry*)((void *)0))->link))) - ((char *) ((void *)0))))); } while (0);
        delcount = 0;
        unsolicited = 0;
        dowork = 0;

        switch (intelligent) {
        case 0:
            dowork = 1;
            break;

        case 1:
            retries = 10;
            status = 0;

            do {
                if (status != 0) {
                    apr_sleep(5000);
                }
                status = apr_stat(&info, path, 0x00000010, instance);
            } while (status != 0 && !interrupted && --retries);

            if (status == 0) {
                previous = info.mtime;
                intelligent = 2;
            }
            dowork = 1;
            break;

        case 2:
            retries = 10;
            status = 0;

            do {
                if (status != 0) {
                    apr_sleep(5000);
                }
                status = apr_stat(&info, path, 0x00000010, instance);
            } while (status != 0 && !interrupted && --retries);

            if (status == 0) {
                if (previous != info.mtime) {
                    dowork = 1;
                }
                previous = info.mtime;
                break;
            }
            intelligent = 1;
            dowork = 1;
            break;
        }

        if (dowork && !interrupted) {
            if (!process_dir(path, instance) && !interrupted) {
                purge(path, instance, max);
            }
            else if (!isdaemon && !interrupted) {
                apr_file_printf(errfile, "An error occurred, cache cleaning "
                                         "aborted." "\n");
                return 1;
            }

            if (intelligent && !interrupted) {
                retries = 10;
                status = 0;
                do {
                    if (status != 0) {
                        apr_sleep(5000);
                    }
                    status = apr_stat(&info, path, 0x00000010, instance);
                } while (status != 0 && !interrupted && --retries);

                if (status == 0) {
                    previous = info.mtime;
                    intelligent = 2;
                }
                else {
                    intelligent = 1;
                }
            }
        }

        apr_pool_destroy(instance);

        current = apr_time_now();
        if (current < now) {
            delay = repeat;
        }
        else if (current - now >= repeat) {
            delay = repeat;
        }
        else {
            delay = now + repeat - current;
        }







        if (isdaemon) {
            while (delay && !interrupted) {
                if (delay > 1000000L) {
                    apr_sleep(1000000L);
                    delay -= 1000000L;
                }
                else {
                    apr_sleep(delay);
                    delay = 0;
                }
            }
        }
    } while (isdaemon && !interrupted);

    if (!isdaemon && interrupted) {
        apr_file_printf(errfile, "Cache cleaning aborted due to user "
                                 "request." "\n");
        return 1;
    }

    return 0;
}
